{
  "name": "icon",
  "version": "0.0.0",
  "scripts": {
    "pages": "ng build --project=playground --prod --base-href=https://joster-dev.github.io/icon/",
    "build-lib": "ng build --prod && npm run copy-license",
    "copy-license": "powershell Copy-Item \".\\LICENSE\" -Destination \".\\dist\\icon\"",
    "npm-publish": "cd dist/icon && npm publish --force",
    "package": "npm run build-lib && npm run npm-publish"
  },
  "private": true,
  "dependencies": {
